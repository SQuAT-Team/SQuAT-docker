						</configuration>
					</execution>
					<execution>
						<id>org.eclipse.emf.core</id>
						<phase>initialize</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>org.eclipse.emf</groupId>
							<artifactId>org.eclipse.emf.ecore</artifactId>
							<version>2.10.2</version>
							<packaging>jar</packaging>
							<file>/home/roehrdor/Workspace-oxygen/SQuAT-docker/squat.utils/lib/org.eclipse.emf.ecore_2.10.2.v20150123-0348.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>de.uka.sdq.identifier</id>
						<phase>initialize</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>de.uka.ipd.sdq</groupId>
							<artifactId>de.uka.ipd.sdq.identifier</artifactId>
							<version>2.1.0.201608251324</version>
							<packaging>jar</packaging>
							<file>/home/roehrdor/Workspace-oxygen/SQuAT-docker/squat.utils/lib/de.uka.ipd.sdq.identifier_2.1.0.201608251324.jar</file>
