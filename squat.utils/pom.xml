							<file>/home/roehrdor/Workspace-oxygen/SQuAT-docker/squat.utils/lib/org.eclipse.emf.cdo_4.3.0.v20140520-1823.jar</file>
						</configuration>
					</execution>
					<execution>
						<id>org.eclipse.emf.common</id>
						<phase>initialize</phase>
						<goals>
							<goal>install-file</goal>
						</goals>
						<configuration>
							<groupId>org.eclipse.emf</groupId>
							<artifactId>org.eclipse.emf.common</artifactId>
